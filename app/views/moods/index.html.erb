<div class="row">
 <div class="small-2 large-4 columns">
		<% @moods.sort_by(&:name).each do |mood| %>
			<div class="mood-box">
				<img src= @mood.image>
				<h2><%= link_to(mood.name, mood) %></h2>
			</div>
		<% end %>
  </div>
  <div class="small-4 large-4 columns"></div>
  <div class="small-6 large-4 columns"></div>
</div>
